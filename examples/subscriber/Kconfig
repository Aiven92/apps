config EXAMPLES_SUBSCRIBER
	bool "uROS example"
	default n
	depends on UROS
	---help---
		Enable the subscriber uROS example

if EXAMPLES_SUBSCRIBER

config EXAMPLES_SUBSCRIBER_PROGNAME
	string "Program name"
	default "subscriber"
	depends on BUILD_KERNEL
	---help---
		This is the name of the program that will be use when the NSH ELF
		program is installed.

